{
  "name": "rest-express",
  "version": "1.6.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "dev:no-sync": "NODE_ENV=development ENABLE_AUTO_SYNC=false tsx server/index.ts",
    "dev:force-sync": "NODE_ENV=development ENABLE_AUTO_SYNC=true tsx server/index.ts",
    "start": "NODE_ENV=production tsx server/index.ts",
    "start:sync": "NODE_ENV=production ENABLE_AUTO_SYNC=true tsx server/index.ts",
    "integration-engine": "node integration-engine-api.js",
    "integration-engine:dev": "node integration-engine-api.js",
    "build": "vite build",
    "build:check": "(tsc -p tsconfig.json || echo 'TypeScript check completed with warnings') && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest --runInBand",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run --config vitest.config.ts",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:live": "vitest run tests/integration/live-server-tests.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:coverage": "vitest --coverage",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:live:simple": "node tests/integration/test-live-server.js",
    "test:live:comprehensive": "node tests/integration/comprehensive-live-server.js",
    "test:api": "newman run tests/api/postman-collection.json -e tests/api/environment.json",
    "test:visual": "percy exec -- playwright test",
    "test:ai": "node tests/ai-agent/autonomous-tester.js",
    "test:load": "k6 run tests/load/load-test.js",
    "db:migrate": "drizzle-kit push:pg",
    "db:generate": "drizzle-kit generate:pg",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx server/seed.ts",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install || echo 'Husky install failed - continuing anyway'",
    "generate-secrets": "node scripts/generate-secrets.js",
    "playwright:install": "playwright install",
    "sync:local": "bash scripts/sync-local-dev.sh",
    "sync:production": "node scripts/comprehensive-sync-production.cjs",
    "sync:drizzle": "node scripts/sync-schema-from-drizzle.cjs",
    "db:sync": "npm run sync:production",
    "db:reset": "bash scripts/sync-local-dev.sh",
    "license:recalculate": "node scripts/recalculate-license-pools.cjs",
    "sync-navigation": "tsx scripts/sync-navigation-to-db.ts",
    "check-navigation": "tsx scripts/check-navigation-status.ts"
  },
